{
  "name": "@phetw/awesome-panel",
  "version": "2.0.0",
  "description": "An example of lit-element setup that works on IE11",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "bundlewatch": "bundlewatch",
    "start": "web-dev-server --open demo/ --node-resolve --esbuild-target auto",
    "test": "web-test-runner test/**/*.test.js --coverage --node-resolve",
    "test:watch": "web-test-runner test/**/*.test.js --node-resolve --watch"
  },
  "keywords": [
    "webcomponent",
    "lit-element",
    "example"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@phetw"
  },
  "author": "Wasuwat Limsuparhat <wasuwat.se@gmail.com>",
  "license": "MIT",
  "bundlewatch": {
    "files": [
      {
        "path": "build/*.js"
      }
    ]
  },
  "dependencies": {
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.29",
    "@web/dev-server": "0.0.15",
    "@web/test-runner": "^0.9.4",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "bundlewatch": "^0.3.3",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.1"
  }
}
