{
  "name": "awesome-panel",
  "version": "1.1.1",
  "description": "An example of lit-element setup that works on IE11",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "start": "web-dev-server --open demo/ --node-resolve",
    "test": "web-test-runner test/**/*.test.js --coverage --node-resolve",
    "test:watch": "web-test-runner test/**/*.test.js --node-resolve --watch"
  },
  "keywords": [
    "webcomponent",
    "lit-element",
    "ie11",
    "sample",
    "example"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@phetw"
  },
  "author": "Wasuwat Limsuparhat <wasuwat.se@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@open-wc/testing": "^2.5.29",
    "@web/dev-server": "0.0.15",
    "@web/test-runner": "^0.9.4",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.1"
  }
}
